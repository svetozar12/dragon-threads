{
  "name": "shared-sdk",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/sdk/src",
  "projectType": "library",
  "tags": [],
  "implicitDependencies": ["api"],
  "targets": {
    "build": {
      "dependsOn": ["build-openAPI"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/shared/sdk",
        "commands": [
          "npx openapi-generator-cli generate -i http://127.0.0.1:3333/v1/swagger/doc.json -o openapi -g go --additional-properties=isGoSubmodule=true,packageName=sdk",
          "bash init.sh"
        ],
        "parallel": false
      },
      "outputs": ["{projectRoot}/dist"],
      "inputs": ["{projectRoot}/*.proto"]
    }
  }
}
